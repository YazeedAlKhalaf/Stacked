{
  "name": "stacked",
  "displayName": "Stacked",
  "description": "",
  "version": "1.0.0",
  "publisher": "YazeedAlKhalaf",
  "icon": "assets/logo.png",
  "bugs": {
    "url": "https://github.com/YazeedAlKhalaf/Stacked/issues",
    "email": "yazeedfady@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YazeedAlKhalaf/Stacked"
  },
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "dart",
    "flutter",
    "stacked",
    "state-management"
  ],
  "activationEvents": [
    "onCommand:extension.new-stacked-skeleton",
    "onCommand:extension.new-stacked-view-viewmodel",
    "workspaceContains:**/pubspec.yaml"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.new-stacked-view-viewmodel",
        "title": "Stacked: New Stacked View-ViewModel"
      },
      {
        "command": "extension.new-stacked-skeleton",
        "title": "Stacked: New Stacked Skeleton"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.new-stacked-view-viewmodel",
          "group": "stackedGroup@1",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "extension.new-stacked-skeleton",
          "group": "stackedGroup@1",
          "when": "explorerResourceIsFolder"
        }
      ]
    },
    "snippets": [
      {
        "language": "dart",
        "path": "./src/snippets/dart.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.45.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@types/js-yaml": "^3.12.4",
    "@types/lodash": "^4.14.155",
    "@types/mkdirp": "^0.5.1",
    "@types/semver": "^7.2.0",
    "@types/shelljs": "^0.8.8",
    "change-case": "^3.1.0"
  }
}
