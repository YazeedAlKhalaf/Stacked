{
  "name": "stacked",
  "displayName": "Stacked",
  "description": "Stacked is a package for Stacked Architecture",
  "version": "2.2.0",
  "publisher": "YazeedAlKhalaf",
  "icon": "assets/logo.png",
  "bugs": {
    "url": "https://github.com/YazeedAlKhalaf/Stacked/issues",
    "email": "yazeedfady@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YazeedAlKhalaf/Stacked"
  },
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "dart",
    "flutter",
    "stacked",
    "state-management"
  ],
  "activationEvents": [
    "onCommand:extension.new-stacked-skeleton",
    "onCommand:extension.new-stacked-view-viewmodel",
    "workspaceContains:**/pubspec.yaml"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.new-stacked-view-viewmodel",
        "title": "Stacked: New Stacked View-ViewModel"
      },
      {
        "command": "extension.new-stacked-skeleton",
        "title": "Stacked: New Stacked Skeleton"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.new-stacked-view-viewmodel",
          "group": "stackedGroup@1",
          "when": "explorerResourceIsFolder"
        }
      ]
    },
    "snippets": [
      {
        "language": "dart",
        "path": "./snippets/dart.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.56.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.0",
    "eslint": "^7.27.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.1",
    "@types/lodash": "^4.14.170",
    "@types/mkdirp": "^1.0.1",
    "@types/semver": "^7.3.6",
    "@types/shelljs": "^0.8.8",
    "change-case": "^4.1.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "semver": "^7.3.5",
    "shelljs": "^0.8.4"
  }
}
